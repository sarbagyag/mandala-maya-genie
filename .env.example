# LLM
ANTHROPIC_API_KEY=your_key_here
LLM_PROVIDER=anthropic  # options: anthropic | openai | gemini | claude-code

# Embeddings (always Gemini, regardless of LLM_PROVIDER)
GOOGLE_API_KEY=your_key_here

# Claude Code OpenAI-compat proxy (used when LLM_PROVIDER=claude-code)
CLAUDE_CODE_BASE_URL=https://claude.mandalafoods.co/v1
CLAUDE_CODE_API_KEY=your_key_here
CLAUDE_CODE_MODEL=claude-sonnet-4-6

# Database
DATABASE_URL=postgresql://postgres:password@db.supabase.co:5432/postgres

# Redis
REDIS_URL=redis://redis:6379

# Security
ADMIN_TOKEN=generate_a_strong_random_token
